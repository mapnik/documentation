<!doctype html>
<html>
<head>
  <meta charset='utf-8' />
  <title>{{options.name}} {{options.version}} | Documentation</title>
  <meta name='viewport' content='width=device-width,initial-scale=1'>
  <link href='assets/mapnik.css' type='text/css' rel='stylesheet' />
  <link href='assets/highlight.css' type='text/css' rel='stylesheet' />
</head>
<body class='documentation cf'>
  
  <div class='toc-container'>
    <h1 class='no-anchor title'>{{options.name}}</h1>
    <div class='mb1'><code>{{options.version}}</code></div>
    <input
      placeholder='Filter'
      id='filter-input'
      class='search'
      type='text' />
    <div id='toc' class='toc'>
      {{#each docs}}
        <a
          href='#{{permalink}}'
          class='class'>
          {{name}}
        </a>
        {{#each members.static}}
          <a
            href='#{{permalink}}'
            class=''>
            .{{name}}
          </a>
        {{/each}}
        {{#each members.instance}}
          <a
            href='#{{permalink}}'
            class=' '>
            #{{name}}
          </a>
        {{/each}}
        {{#each events}}
          <a
            href='#{{permalink}}'
            class=''>
            <code>â“” {{name}}</code>
          </a>
        {{/each}}
      {{/each}}
    </div>
  </div>

  <div class='content-container'>
    <div class='content'>
    {{#each docs ~}}<div class='main'>{{> section}}</div>{{/each}}
    </div>
  </div>

  <script src='assets/anchor.js'></script>
  <script src='assets/site.js'></script>
</body>
</html>
